                        "1": {
                            "name": "punctuation.definition.entity.chuanqi"
                        },
                        "3": {
                            "name": "punctuation.definition.entity.chuanqi"
                        }
                    },
                    "match": "(\\<)(.*?)(\\>)",
                    "name": "entity.name.section.group-title.chuanqi"
                },
                {
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.entity.chuanqi"
                        },
                        "3": {
                            "name": "punctuation.definition.entity.chuanqi"
                        }
                    },
                    "match": "(\\【)(.*?)(\\】)",
                    "name": "entity.name.section.group-titlecn.chuanqi"
