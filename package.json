{
    "name": "chuanqi-script-lang",
    "publisher": "itellyou-com",
    "displayName": "ChuanQi Script Lang",
    "description": "传奇脚本语言",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.48.0"
    },
